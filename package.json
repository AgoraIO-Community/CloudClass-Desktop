{
  "name": "root",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "dayjs": "1.10.4",
    "lerna": "3.22.1",
    "npm-run-all": "4.1.5",
    "typescript": "^4.1.3"
  },
  "agora_electron": {
    "electron_version": "7.1.2",
    "prebuilt": true
  },
  "description": "[English](README.zh.md)*",
  "version": "1.0.0",
  "dependencies": {},
  "scripts": {
    "bootstrap": "lerna bootstrap --use-workspaces",
    "release:classroom:sdk": "run-s build:sdk build:ui-kit build:classroom:sdk",
    "release:electron:mac": "run-s build:sdk build:ui-kit pack:electron:mac",
    "release:electron:win": "run-s build:sdk build:ui-kit pack:electron:win",
    "release:demo": "run-s build:sdk build:ui-kit build:demo",
    "dev": "run-s build:sdk build:ui-kit run:demo",
    "dev:electron": "run-s build:dev:sdk build:ui-kit run:demo:electron",
    "dev:aclass-ui-kit": "lerna exec --scope=agora-aclass-ui-kit yarn storybook",
    "dev:ui-kit": "lerna exec --scope=agora-scenario-ui-kit yarn storybook",
    "pack:sdk": "run-s build:sdk build:ui-kit build:classroom:sdk",
    "pack:electron:mac": "lerna exec --scope=agora-classroom-sdk npm run pack:mac",
    "pack:electron:win": "lerna exec --scope=agora-classroom-sdk npm run pack:win",
    "build:classroom:sdk": "lerna exec --scope=agora-classroom-sdk npm run pack:npm",
    "build:dev:sdk": "lerna exec --scope=agora-rte-sdk npm run build:dev:js",
    "build:sdk": "lerna exec --scope=agora-rte-sdk npm run build",
    "build:aclass-ui-kit": "lerna exec --scope=agora-aclass-ui-kit npm run build",
    "build:ui-kit": "lerna exec --scope=agora-scenario-ui-kit npm run build",
    "build:demo": "lerna exec --scope=agora-classroom-sdk npm run build",
    "run:build:sdk": "run-s build:sdk build:ui-kit",
    "run:demo": "lerna exec --scope=agora-classroom-sdk npm run dev",
    "run:demo:electron": "lerna exec --scope=agora-classroom-sdk npm run electron",
    "run:apaas": "lerna exec --scope=agora-apaas-demo npm run dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agoraio-community/CloudClass-Desktop.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/agoraio-community/CloudClass-Desktop/issues"
  },
  "homepage": "https://github.com/agoraio-community/CloudClass-Desktop#readme"
}

{
  "name": "Agora-Edu-Web",
  "version": "6.2.0",
  "private": true,
  "homepage": ".",
  "main": "./src/index.tsx",
  "agora_electron": {
    "platform": "darwin",
    "electron_version": "7.1.2",
    "prebuilt": true,
    "arch": "x64"
  },
  "dmg": {
    "compression": "store",
    "npmRebuild": false,
    "title": "agora_edu",
    "contents": [
      {
        "x": 130,
        "y": 220
      },
      {
        "x": 410,
        "y": 220,
        "type": "link",
        "path": "/Applications"
      }
    ]
  },
  "build": {
    "extraMetadata": {
      "main": "build/index.js"
    },
    "productName": "AgoraEducation",
    "appId": "io.agora.education",
    "files": [
      "build/**/*"
    ],
    "artifactName": "${productName}-${version}.${ext}",
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "icons/favicon.png",
      "entitlements": "entitlements.mac.plist",
      "category": "io.agora.Education",
      "hardenedRuntime": true,
      "extendInfo": {
        "NSMicrophoneUsageDescription": "AgoraEdu Classroom acquire your microphone permission",
        "NSCameraUsageDescription": "AgoraEdu Classroom acquire your camera permission"
      }
    },
    "win": {
      "icon": "icons/favicon.png",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32"
          ]
        }
      ]
    },
    "linux": {
      "icon": "icons/favicon.png",
      "target": [
        "deb",
        "rpm",
        "snap",
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "buildResources": "assets",
      "output": "release"
    }
  },
  "scripts": {
    "install_electron": "node ./scripts/postinstall.js",
    "electron": "cross-env REACT_APP_RUNTIME_PLATFORM=electron ELECTRON_START_URL=http://localhost:3000 concurrently \"npm run electron:web\" \"wait-on http://localhost:3000 && npm run electron:client\"",
    "electron:client": "cross-env electron ./app/",
    "electron:web": "cross-env BROWSER=none react-app-rewired start",
    "electron:copy:web": "cross-env REACT_APP_RUNTIME_PLATFORM=electron react-app-rewired build",
    "electron:copy:electron": "cpx ./app/**/*.js ./build",
    "electron:build": "npm run electron:copy:web && npm run electron:copy:electron",
    "dev": "cross-env TSC_COMPILE_ON_ERROR=false REACT_APP_RUNTIME_PLATFORM=web react-app-rewired start",
    "build": "cross-env TSC_COMPILE_ON_ERROR=false REACT_APP_RUNTIME_PLATFORM=web react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "eject": "react-scripts eject",
    "pack:mac": "npm run electron:build && electron-builder --mac",
    "pack:win": "npm run electron:build && electron-builder --win",
    "pack:npm": "cross-env NODE_ENV=production BABEL_NODE=production webpack --mode='production'",
    "doc": "typedoc --o"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "7.7.5",
    "@material-ui/core": "4.9.9",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "4.0.0-alpha.56",
    "@netless/combine-player": "1.1.4",
    "@netless/react-seek-slider": "1.0.4",
    "@netless/white-audio-plugin": "1.2.16",
    "@netless/white-video-plugin": "1.2.15",
    "@types/ali-oss": "6.0.4",
    "@types/axios": "0.14.0",
    "@types/deep-diff": "1.0.0",
    "@types/jest": "24.0.19",
    "@types/joi": "14.3.4",
    "@types/lodash": "4.14.145",
    "@types/node": "12.12.47",
    "@types/raf": "3.4.0",
    "@types/react": "16.9.9",
    "@types/react-beautiful-dnd": "13.0.0",
    "@types/react-color": "3.0.1",
    "@types/react-dom": "16.9.2",
    "@types/react-router-dom": "5.1.0",
    "@types/ua-parser-js": "^0.7.33",
    "@types/uuid": "3.4.6",
    "@types/video.js": "7.3.4",
    "agora-rtc-sdk-ng": "4.1.1",
    "agora-rtm-sdk": "1.4.1",
    "ali-oss": "6.3.1",
    "axios": "0.19.2",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "babel-plugin-emotion": "10.0.33",
    "babel-plugin-import": "1.13.0",
    "cache-loader": "4.1.0",
    "concurrently": "5.0.0",
    "cpx": "1.5.0",
    "cross-env": "6.0.3",
    "css-loader": "2.1.0",
    "customize-cra": "0.8.0",
    "deep-diff": "1.0.2",
    "devtron": "1.4.0",
    "dexie": "3.0.2",
    "electron": "7.1.14",
    "electron-builder": "22.5.1",
    "eruda": "^2.2.1",
    "eslint-plugin-react": "7.20.0",
    "glob-all": "3.2.1",
    "hard-source-webpack-plugin": "0.13.1",
    "immutable": "4.0.0-rc.12",
    "joi": "17.3.0",
    "js-md5": "0.7.3",
    "lodash": "4.17.15",
    "mini-css-extract-plugin": "1.3.3",
    "mobx": "5.15.4",
    "mobx-react": "6.3.1",
    "moment": "2.24.0",
    "node-sass": "4.12.0",
    "progress-bar-webpack-plugin": "2.1.0",
    "promise-polyfill": "8.2.0",
    "purify-css": "1.2.5",
    "purifycss-webpack": "0.7.0",
    "react": "16.13.1",
    "react-app-rewired": "2.1.4",
    "react-beautiful-dnd": "13.0.0",
    "react-color": "2.17.3",
    "react-dom": "16.13.1",
    "react-draggable": "4.2.0",
    "react-gtm-module": "2.0.8",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.2.0",
    "react-use": "13.24.0",
    "rxjs": "6.5.3",
    "simple-progress-webpack-plugin": "1.1.2",
    "source-map-support": "0.5.16",
    "speed-measure-webpack-plugin": "1.3.3",
    "thread-loader": "2.1.3",
    "typedoc": "0.19.2",
    "typescript": "3.8.3",
    "ua-parser-js": "0.7.22",
    "uuid": "3.3.3",
    "video.js": "7.7.5",
    "wait-on": "^3.3.0",
    "webpack-bundle-analyzer": "3.8.0",
    "webpack-cli": "4.3.1",
    "white-web-sdk": "2.11.9",
    "worker-loader": "2.0.0",
    "zlib": "1.0.5"
  },
  "dependencies": {
    "adm-zip": "0.4.14",
    "agora-electron-sdk": "education290",
    "bowser": "1.9.4",
    "react-beautiful-dnd": "13.0.0"
  }
}

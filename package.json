{
  "name": "root",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "resolutions": {
    "@babel/core": "7.13.15",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "7.13.8",
    "@babel/plugin-syntax-decorators": "7.12.13",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.13.15",
    "@babel/preset-env": "7.13.15",
    "@babel/preset-react": "7.13.13",
    "@babel/preset-typescript": "7.13.0",
    "@babel/runtime-corejs3": "7.13.10"
  },
  "devDependencies": {
    "dayjs": "1.10.4",
    "just-scripts": "1.5.3",
    "lerna": "3.22.1",
    "npm-run-all": "4.1.5",
    "typescript": "4.2.3"
  },
  "agora_electron": {
    "electron_version": "12.0.0",
    "prebuilt": true
  },
  "description": "[English](README.zh.md)*",
  "version": "1.0.0",
  "scripts": {
    "bootstrap": "lerna bootstrap --use-workspaces",
    "release:apaas:sdk": "lerna exec --scope=agora-classroom-sdk npm run pack:npm",
    "release:classroom:sdk": "run-s build:proto release:apaas:sdk",
    "release:electron:mac": "run-s release:demo pack:electron:mac",
    "release:electron:win": "run-s release:demo pack:electron:win",
    "release:edu-core": "run-s build:rte-sdk build:edu-core",
    "release:demo": "run-s build:apaas",
    "dev": "run-s build:proto run:demo",
    "dev:apaas": "run-s build:proto run:demo",
    "build:apaas": "run-s build:proto build:demo",
    "build:proto": "lerna exec --scope=agora-edu-core npm run proto",
    "dev:electron": "run-s build:proto run:demo:electron",
    "dev:ui-kit": "lerna exec --scope=agora-scenario-ui-kit yarn storybook",
    "dev:plugin": "lerna exec --scope=agora-plugin-gallery npm run dev",
    "build:dev:plugin": "lerna exec --scope=agora-plugin-gallery npm run build:dev",
    "pack:sdk": "run-s build:rte-sdk build:classroom:sdk",
    "pack:electron:mac": "lerna exec --scope=agora-classroom-sdk npm run pack:mac",
    "pack:electron:win": "lerna exec --scope=agora-classroom-sdk npm run pack:win",
    "build:classroom:sdk": "lerna exec --scope=agora-classroom-sdk npm run pack:npm",
    "build:dev:rte-sdk": "lerna exec --scope=agora-rte-sdk npm run build:dev",
    "build:chat-widget": "lerna exec --scope=agora-chat-widget yarn build:lib",
    "build:rte-sdk": "lerna exec --scope=agora-rte-sdk npm run build",
    "build:plugin": "lerna exec --scope=agora-plugin-gallery npm run build",
    "build:dev:widget": "lerna exec --scope=agora-widget-gallery npm run build:dev",
    "build:widget": "lerna exec --scope=agora-widget-gallery npm run build",
    "build:dev:edu-core": "lerna exec --scope=agora-edu-core npm run build:dev:js",
    "build:edu-core": "lerna exec --scope=agora-edu-core npm run build:js",
    "build:demo": "lerna exec --scope=agora-classroom-sdk npm run build",
    "run:build:rte-sdk": "run-s build:rte-sdk",
    "run:demo": "lerna exec --scope=agora-classroom-sdk npm run dev",
    "run:demo:electron": "lerna exec --scope=agora-classroom-sdk npm run electron",
    "run:apaas": "lerna exec --scope=agora-apaas-demo npm run dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "ci:build:web": "yarn release:demo",
    "ci:build:electron:win": "yarn release:electron:win",
    "ci:build:electron:mac": "yarn release:electron:mac",
    "ci:build:classroom:sdk": "yarn release:classroom:sdk"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agoraio-community/CloudClass-Desktop.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/agoraio-community/CloudClass-Desktop/issues"
  },
  "homepage": "https://github.com/agoraio-community/CloudClass-Desktop#readme",
  "dependencies": {
    "agora-edu-core": "1.1.2",
    "type-fest": "^1.1.3"
  }
}

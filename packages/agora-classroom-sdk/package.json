    "@zip.js/zip.js": "2.2.22",
    "agora-access-token": "2.0.4",
    "ali-oss": "6.15.2",
    "animate.css": "4.1.1",
    "autoprefixer": "10.2.5",
    "axios": "0.19.2",
    "bowser": "1.9.4",
    "broadcast-channel": "3.7.0",
    "classnames": "2.2.6",
    "concurrently": "5.0.0",
    "cpx": "1.5.0",
    "cross-env": "6.0.3",
    "dayjs": "1.10.4",
    "deep-diff": "1.0.2",
    "devtron": "1.4.0",
    "dexie": "3.0.2",
    "eruda": "2.2.1",
    "extract-zip": "2.0.1",

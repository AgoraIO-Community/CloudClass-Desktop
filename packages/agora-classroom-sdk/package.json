{
  "name": "agora-classroom-sdk",
  "version": "2.5.1",
  "homepage": ".",
  "sideEffects": [
    "*.css"
  ],
  "keywords": [
    "agora",
    "WebRTC",
    "latest",
    "AgoraFlexibleClassroom",
    "Education"
  ],
  "main": "lib/edu_sdk.bundle.js",
  "types": "lib/agora-classroom-sdk/src/infra/api/index.d.ts",
  "swSrcPath": "./src/infra/sw/service-worker.ts",
  "agora_electron": {
    "electron_version": "12.0.0",
    "prebuilt": true
  },
  "dmg": {
    "compression": "store",
    "npmRebuild": false,
    "title": "agora_edu",
    "contents": [
      {
        "x": 130,
        "y": 220
      },
      {
        "x": 410,
        "y": 220,
        "type": "link",
        "path": "/Applications"
      }
    ]
  },
  "build": {
    "asar": true,
    "asarUnpack": [
      "node_modules/agora-rdc-core/**/*"
    ],
    "afterPack": "./after-pack.js",
    "extraMetadata": {
      "main": "build/index.js"
    },
    "productName": "AgoraFlexibleClassroom",
    "appId": "io.agora.education",
    "files": [
      "build/**/*"
    ],
    "extraResources": [
      {
        "from": "./assets/pretest-audio.mp3",
        "to": "pretest-audio.mp3"
      }
    ],
    "artifactName": "${productName}-${version}.${ext}",
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "icons/favicon.png",
      "entitlements": "entitlements.mac.plist",
      "category": "io.agora.Education",
      "hardenedRuntime": true,
      "extendInfo": {
        "NSMicrophoneUsageDescription": "AgoraEdu Classroom acquire your microphone permission",
        "NSCameraUsageDescription": "AgoraEdu Classroom acquire your camera permission"
      }
    },
    "win": {
      "icon": "icons/favicon.png",
      "target": [
        "nsis"
      ],
      "requestedExecutionLevel": "requireAdministrator"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "icons/favicon.ico",
      "shortcutName": "AgoraFlexibleClassroom",
      "artifactName": "AgoraFlexibleClassroom-${version}.${ext}"
    },
    "linux": {
      "icon": "icons/favicon.png",
      "target": [
        "deb",
        "rpm",
        "snap",
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "buildResources": "assets",
      "output": "release"
    }
  },
  "scripts": {
    "electron": "cross-env NODE_ENV=development RTE_RUNTIME_PLATFORM=electron ELECTRON_START_URL=http://localhost:3000 concurrently \"npm run dev\" \"wait-on http://localhost:3000 && electron ./app/\"",
    "electron:client": "cross-env NODE_ENV=development ELECTRON_START_URL=http://localhost:3000 electron ./app/",
    "electron:copy:electron:mac": "cpx './app/**/*.js' ./build",
    "electron:copy:electron:win": "cpx ./app/**/*.js ./build",
    "dev": "cross-env NODE_OPTIONS=--max_old_space_size=8192 webpack serve --config ./webpack/webpack.dev.js",
    "build": "cross-env NODE_OPTIONS=--max_old_space_size=8192 webpack --config ./webpack/webpack.prod.js",
    "build:types": "tsc -p tsconfig.json",
    "build:docs": "npx typedoc",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "pack:mac": "yarn electron:copy:electron:mac && electron-builder --mac",
    "pack:win": "yarn electron:copy:electron:win && electron-builder --win",
    "pack:npm": "run-s bundle:npm build:types",
    "bundle:npm": "cross-env NODE_OPTIONS=--max_old_space_size=8192 webpack --config ./webpack/webpack.pack.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "agora-electron-sdk": "*"
  },
  "peerDependenciesMeta": {
    "agora-electron-sdk": {
      "optional": true
    }
  },
  "devDependencies": {
    "@netless/fetch-progress": "0.0.1",
    "@netless/iframe-bridge": "2.1.4",
    "@tailwindcss/forms": "0.2.1",
    "@types/ali-oss": "6.0.7",
    "@types/axios": "0.14.0",
    "@types/classnames": "2.2.11",
    "@types/deep-diff": "1.0.0",
    "@types/extract-zip": "1.6.2",
    "@types/jest": "24.0.19",
    "@types/joi": "14.3.4",
    "@types/lodash": "4.14.145",
    "@types/node": "12.12.47",
    "@types/raf": "3.4.0",
    "@types/react": "17.0.44",
    "@types/react-datepicker": "3.1.8",
    "@types/react-dom": "16.9.2",
    "@types/react-helmet": "6.1.5",
    "@types/react-router-dom": "5.3.3",
    "@types/react-select": "4.0.15",
    "@types/react-transition-group": "4.4.1",
    "@types/ua-parser-js": "0.7.33",
    "@types/uuid": "3.4.6",
    "@types/video.js": "7.3.4",
    "@types/workbox-core": "4.3.0",
    "@types/workbox-routing": "4.3.0",
    "@types/workbox-sw": "4.3.1",
    "electron": "12.0.0",
    "electron-builder": "22.11.7",
    "electron-util": "0.16.0"
  },
  "dependencies": {
    "adm-zip": "0.4.14",
